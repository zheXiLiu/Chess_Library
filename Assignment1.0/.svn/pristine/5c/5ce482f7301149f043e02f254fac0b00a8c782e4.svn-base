package chess.junit;

/**
 * @author Zhexi
 * Square class represent a unit cell in board[x][y]
 */
public class Square
{
	public int x;
	public int y;
	Piece attacker;  
	
	/**
	 * @param x x coordinate
	 * @param y y coordinate
	 * @param attacker square(x,y) under the attacking/movable range of that piece
	 */
	public Square(int x, int y, Piece attacker)
	{
		this.x= x;
		this.y =y;
		this.attacker = attacker;
	}
}
